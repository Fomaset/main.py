a = []
b=0
c=1
print("Введите натуральные числа. Чтобы прекратить ввод введите любое не натуральное число")
while c != 0:
    c = float(input())
    if c % 1 == 0 and c > 0:
        a.append(int(c))
        b+=1
    else:
        print("Число не является натуральным")
for i in range(b-1):
    for x in range(len(a)-1):
        if a[x] > a[x+1]:
            a[x], a[x+1] = a[x+1], a[x]
print(a)
